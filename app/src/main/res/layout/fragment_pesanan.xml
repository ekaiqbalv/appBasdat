<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".PesananFragment"
    android:background="#e6eaed">

    <!-- TODO: Update blank fragment layout -->
    <android.support.design.widget.BottomNavigationView
        android:id="@+id/navigationPesanan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="0dp"
        android:layout_marginStart="0dp"
        app:itemIconTint="@drawable/botnav_selector"
        app:itemTextColor="@drawable/botnav_selector"
        android:background="?android:attr/windowBackground"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/menu_fragment_pesanan" />

    <FrameLayout
        android:id="@+id/fragment_pesanan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/navigationPesanan">
    </FrameLayout>

</android.support.constraint.ConstraintLayout>